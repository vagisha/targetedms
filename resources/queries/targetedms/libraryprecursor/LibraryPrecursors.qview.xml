<customView xmlns="http://labkey.org/data/xml/queryCustomView" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        label="Library Precursors">
    <columns>
        <column name="PeptideId/PeptideGroupId/RunId/File" />
        <column name="PeptideId/PeptideGroupId/Label" />
        <column name="ModifiedPrecursorDisplayColumn" /> <!-- Defined in ModifiedSequenceDisplayColumn.java -->
        <column name="Charge" />
        <column name="IsotopeLabelId" />
        <column name="Mz" />
        <column name="NeutralMass" />
        <column name="CollisionEnergy" />
        <column name="DeclusteringPotential" />
        <column name="NoteAnnotations" />
    </columns>

    <filters>
        <filter column="RepresentativeDataState" operator="eq" value="1"/>
    </filters>

    <sorts>
       <sort column="PeptideId/PeptideGroupId/Label" />
       <sort column="ModifiedSequence" />
       <sort column="PeptideId/PeptideGroupId/RunId/File" />
   </sorts>

</customView>