<customView xmlns="http://labkey.org/data/xml/queryCustomView" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        label="Library Molecules" canOverride="true">
    <columns>
        <column name="PeptideGroupId/RunId/File" />
        <column name="Molecule" />
        <column name="IonFormula" />
        <column name="CustomIonName" />
        <column name="MassMonoisotopic" />
        <column name="MassAverage" />
        <column name="RtCalculatorScore" />
        <column name="PredictedRetentionTime" />
        <column name="AvgMeasuredRetentionTime" />
        <column name="NoteAnnotations" />
    </columns>
    <filters>
        <filter column="RepresentivePrecursorCount" operator="gt" value="0"/>
    </filters>
    <sorts>
       <sort column="Molecule" />
       <sort column="IonFormula" />
       <sort column="CustomIonName" />
       <sort column="PeptideGroupId/RunId/File" />
    </sorts>
</customView>