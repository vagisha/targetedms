<customView xmlns="http://labkey.org/data/xml/queryCustomView" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            label="Library Precursors" canOverride="true">
    <columns>
        <column name="MoleculeId/PeptideGroupId/Label" />
        <column name="MoleculeId/Molecule" />
        <column name="CustomIonName" />
        <column name="IonFormula" />
        <column name="Charge" />
        <column name="IsotopeLabelId" />
        <column name="Mz" />
        <column name="MassMonoisotopic" />
        <column name="MassAverage" />
        <column name="CollisionEnergy" />
        <column name="DeclusteringPotential" />
        <column name="NoteAnnotations" />
        <column name="MoleculeId/PeptideGroupId/RunId/File" />
    </columns>

    <filters>
        <filter column="RepresentativeDataState" operator="eq" value="1"/>
    </filters>

    <sorts>
        <sort column="MoleculeId/MoleculeName" />
        <sort column="IonFormula" />
        <sort column="CustomIonName" />
        <sort column="MoleculeId/PeptideGroupId/RunId/File" />
    </sorts>

</customView>